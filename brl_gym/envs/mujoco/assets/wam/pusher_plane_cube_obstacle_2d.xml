                                                        <geom rgba="0 0 0.5 0" type="mesh" mesh="finger2_2_collision" contype="1" conaffinity="1"/>
                                                    </body>
                                                </body>
<!--
                                                <joint name="push_slidex" type="slide" pos="0 0 0" axis="1 0 0" range="-10.3213 10.3" damping="0.1"/>
                                                <joint name="push_slidey" type="slide" pos="0 0 0" axis="0 1 0" range="-10.3213 10.3" damping="0.1"/>
                                                <joint name="push_hinge" type="hinge" pos="0 0 0" axis="0 0 1" range="-3.2 3.2" damping="0.1"/>
 -->
                                                <body name="object" pos="-0.00 0.0 0.15" euler="0. 1.5707 0.">
                                                    <geom rgba="0 1 0 1" type="cylinder" size="0.06 0.04 0.04" contype="1" conaffinity="1"/>
<!--
                                                <joint name="object_slidex" type="slide" pos="0 0 0" axis="1 0 0" range="-10.3213 10.3" damping="0.1"/>
                                                <joint name="object_slidey" type="slide" pos="0 0 0" axis="0 1 0" range="-10.3213 10.3" damping="0.1"/>
                                                <joint name="object_slidez" type="hinge" pos="0 0 0" axis="0 0 1" range="-3.2 3.2" damping="0.1"/>
 -->
                                                </body>
                                            </body>
                                        </body>
                                    </body>
                                </body>
                            </body>
                        </body>
                    </body>
                </body>
            </body>
        </body>

        <camera euler="0 0 0" fovy="50" name="top_cam" pos="0 0 2.0"></camera>
    </worldbody>
    <!-- <equality>
        <weld body1="robot0:mocap" body2="barrett_hand" solimp="0.9 0.95 0.001" solref="0.02 1"></weld>
    </equality> -->
    <actuator>
<!--         <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="200.0" joint="object_slidex"/>
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="200.0" joint="object_slidey"/>
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="200.0" joint="object_slidez"/> -->
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="5.0" joint="wam1_joint"/>
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="5.0" joint="wam2_joint"/>
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="5.0" joint="wam3_joint"/>
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="5.0" joint="wam4_joint"/>
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" gear="5.0" joint="wam5_joint"/>
