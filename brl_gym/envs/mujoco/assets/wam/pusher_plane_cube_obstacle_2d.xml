    <compiler angle="radian" inertiafromgeom="true"/>
    <default>
        <joint armature="1" damping="1" limited="true"/>
        <geom contype="0" friction="0.5 0.5 0.1" rgba="0.7 0.7 0 1" solimp=".5 .5 .001" solref=".02 1"/>
    </default>
    <option integrator="RK4" timestep="0.01"/>
    <worldbody>
        <body mocap="true" name="robot0:mocap" pos="0 0 0">
            <geom conaffinity="0" contype="0" pos="0 0 0" rgba="0 0.5 0 0.7" size="0.005 0.005 0.005" type="box"></geom>
            <geom conaffinity="0" contype="0" pos="0 0 0" rgba="0 0.5 0 0.1" size="1 0.005 0.005" type="box"></geom>
            <geom conaffinity="0" contype="0" pos="0 0 0" rgba="0 0.5 0 0.1" size="0.005 1 0.001" type="box"></geom>
            <geom conaffinity="0" contype="0" pos="0 0 0" rgba="0 0.5 0 0.1" size="0.005 0.005 1" type="box"></geom>
        </body>
        <!-- Ground -->
        <geom conaffinity="1" contype="1" name="ground" pos="0 0 0" rgba="0.9 0.9 0.9 1" size="1 1 10" type="plane" condim="3" friction="0.01 0.01 0.01"/>
        <!-- Ground Center -->
        <body name="center" pos="0. 0. 0.">
            <geom conaffinity="0" contype="0" name="center" pos="0 0 0" rgba="0. 0. 1.0 1" size=".009" type="sphere"/>
        </body>
        <!-- WAM Arm-->
        <body name='pusher' pos="-1. 0. 0.4" euler="0. 0. 0.">
            <inertial diaginertia="1 1 1" mass="1.0" pos="0. 0 0."></inertial>
            <body name='wam' pos="0. 0. 0." euler="0. 1.5708 3.14159">
                <geom rgba="1 0 0 1" type="mesh" mesh="wam_base" contype="0" conaffinity="0" mass="0.001"/>
                <geom rgba="0 0 0.5 0" type="mesh" mesh="wam_base_collision" contype="0" conaffinity="0" mass="0.001"/>
                <body name='wam_1' pos="0.22 0.14 0.346" euler="0. 0 0.5">
                    <joint name="wam1_joint" type="hinge" pos="0 0 0" axis="0 0 1" range="-10.3213 10.3" damping="0."/>
                    <geom rgba="1 0 0 1" type="mesh" mesh="wam1" contype="0" conaffinity="0" mass="0.001"/>
                    <geom rgba="0 0 0.5 0" type="mesh" mesh="wam1_collision" contype="0" conaffinity="0" mass="0.001"/>
                    <body name='wam_2' pos="0. 0. 0." euler="-1.570796 0 -2.0">
